package eg_casting;

public class CastingDemo {

	public static void main(String[] args) {
		/*
		 * Casting/Type Casting - Vonversion of one data type to another
		 * 2 Types of Casting
		 * Implicit Casting - Taken care by java compiler implicilty when the smaller data type is assigned to the larger data type
		 */
		
		int x = 100;
		float m = x; //implicit casting
		System.out.println("x = " + x);
		System.out.println("m = " + m);

	}

}
